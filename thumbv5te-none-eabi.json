{
    "arch": "arm",
    "cpu": "arm946e-s",
    "llvm-target": "thumbv5te-none-eabi",
    "linker": "arm-none-eabi-g++",
    "env": "newlib",
    "dynamic-linking": false,
    "data-layout": "e-m:e-p:32:32-i64:64-v128:64:128-a:0:32-n32-S64",
    "relocation-model": "static",
    "target-endian": "little",
    "target-pointer-width": "32",
    "target-c-int-width": "32",
    "features": "+soft-float,+strict-align",
    "no-compiler-rt": true,
    "disable-redzone": true,
    "executables": true,
    "morestack": false,
    "linker-flavor": "gcc",
    "linker-is-gnu": true,
    "has-elf-tls": false,
    "atomic-cas": false,
    "target-env": "newlib",
    "target-family":  "unix",
    "has-rpath": false,
    "panic-strategy": "abort",
    "vendor": "nintendo",
    "pre-link-args": {
        "gcc": [
            "-MMD",
            "-MP",
            "-MF",
            "-O2",
            "-march=armv5te",
            "-mtune=arm946e-s",
            "-fomit-frame-pointer",
            "-ffast-math",
            "-mthumb",
            "-mthumb-interwork",
            "-DARM9",
            "-fno-rtti",
            "-fno-exceptions"
        ]
    },
    "post-link-args": {
        "gcc": [
            "-specs=ds_arm9.specs",
            "-g",
            "-mthumb",
            "-mthumb-interwork"
        ]
    }
}
